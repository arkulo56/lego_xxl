    <text class="note_title">魔法时钟</text>
  </view>
</view>

<!-- 分割线 -->
<view class="container">
  <view class="divLine"></view>
</view>

<!--早期教育乐高位置-->
<view class="adv-1">
  <image src="cloud://logo-xxl-3e7925.6c6f-logo-xxl-3e7925-1257948763/page_image/旧版乐高笔记窄版.jpg" class="adv-2-detail" bindtap="stumpOldLego"></image>
</view>
